variable "group-policy-json" {   # Variable to hold the IAM policy JSON for the group
    description = "IAM policy JSON for the IAM group"
 type        = string
  default = <<EOF
{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:*",
                "s3-object-lambda:*"
            ],
            "Resource": "*"
        }
    ]
}
EOF
}

variable "pgp_key" {  # Variable to hold the PGP key for encrypting the IAM user password
  description = "PGP key for encrypting the IAM user password"
  type        = string
  default     = ""
}